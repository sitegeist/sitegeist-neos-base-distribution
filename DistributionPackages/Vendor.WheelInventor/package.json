{
  "name": "@vendor/wheelinventor",
  "version": "1.0.0",
  "scripts": {
    "build": "concurrently 'yarn build:js' 'yarn build:css'",
    "build:js": "webpack --mode production",
    "build:css": "TAILWIND_MODE=build NODE_ENV=production postcss Resources/Private/Styles/styles.css -o Resources/Public/Build/Styles/main.min.css",
    "watch": "concurrently 'yarn watch:js' 'yarn watch:css'",
    "watch:js": "webpack -w --mode development",
    "watch:css": "TAILWIND_MODE=watch NODE_ENV=development postcss -w Resources/Private/Styles/styles.css -o Resources/Public/Build/Styles/main.min.css",
    "lint": "eslint Resources/Private/**/*.ts",
    "test": "echo \"Error: no test specified\""
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "final-form": "^4.20.2",
    "final-form-focus": "^1.1.2",
    "keen-slider": "^5.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.3",
    "rxjs": "^7.3.0"
  },
  "devDependencies": {
    "@sitegeist/eslint-config-es-neos-eslint": "^2.0.2",
    "@tailwindcss/typography": "^0.5.0",
    "@types/classnames": "^2.3.1",
    "@types/final-form-focus": "^1.1.1",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/glob": "^7.1.4",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "autoprefixer": "^10.3.4",
    "concurrently": "^6.2.1",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "glob": "^7.1.7",
    "lerna": "^4.0.0",
    "node": "^16.6.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-discard-duplicates": "^5.0.1",
    "postcss-selector-parser": "^6.0.6",
    "tailwindcss": "^3.0.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  }
